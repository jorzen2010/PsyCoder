@model psycoderEntity.ZixunshiUser
@section Topcss {
    <link href="~/plugins/bootstrap-validator/css/bootstrapValidator.css" rel="stylesheet" type="text/css" />
}
<div class="content">

    <h3 class="pageTitle">
        认证信息 <small>为了保护您的信息安全，本系统不保存您的身份证和执业证照片，人工认证后不可修改</small>
    </h3>
    <div class="box">
        <div class="box-header">
            <h3 class="box-title">
               认证信息
            </h3>
            <div class="pull-right">

                <button class="btn btn-info btn-sm" onclick="alertconfirm('请联系管理员微信：sky20100')"> 申请修改认证信息</button>
                <button class="btn btn-default btn-sm" onclick="alertconfirm('请联系管理员微信：sky20100')"> 申请修注销账号</button>
 
            </div>
        </div>
        <div class="box-body">
            <div class="row">
                <div class="col-md-12">
                    <form id="setting" name="setting">

                        @Html.AntiForgeryToken()
                        @Html.HiddenFor(model => model.Id)
                        <div class="xcxset">

                            <ul>


                                <li>
                                    <div class="form-group">

                                        @Html.LabelFor(model => model.PsyRealName, new { @class = "col-sm-2 Settinglable  no-margin" })

                                        <div class="col-sm-10">
                                            @Html.TextBoxFor(model => model.PsyRealName, new { @class = "SettingInput col-sm-10 ", @readOnly = "true" })

                                        </div>
                                    </div>
                                </li>



                                <li>
                                    <div class="form-group">

                                        @Html.LabelFor(model => model.PsyNumber, new { @class = "col-sm-2 Settinglable  no-margin" })

                                        <div class="col-sm-10">
                                            @Html.TextBoxFor(model => model.PsyNumber, new { @class = "SettingInput col-sm-10 ", @readOnly = "true" })

                                        </div>
                                    </div>
                                </li>

                                <li>
                                    <div class="form-group">

                                        @Html.LabelFor(model => model.PsyZhengshuNumber, new { @class = "col-sm-2 Settinglable  no-margin" })

                                        <div class="col-sm-10">
                                            @Html.TextBoxFor(model => model.PsyZhengshuNumber, new { @class = "SettingInput col-sm-10 ", @readOnly = "true" })

                                        </div>
                                    </div>
                                </li>

                                <li>
                                    <div class="form-group">

                                        @Html.LabelFor(model => model.CreateTime, new { @class = "col-sm-2 Settinglable  no-margin" })

                                        <div class="col-sm-10">
                                            @Html.TextBoxFor(model => model.CreateTime, new { @class = "SettingInput col-sm-10 ", @readOnly = "true" })

                                        </div>
                                    </div>
                                </li>

                                <li>
                                    <div class="form-group">
                                        <label class="col-sm-2 Settinglable  no-margin">注册邮箱</label>

                                        <div class="col-sm-10">
                                            @Html.TextBoxFor(model => model.PsyUserEmail, new { @class = "SettingInput col-sm-10 ", @readOnly = "true" })

                                        </div>
                                    </div>
                                </li>

                                <li>
                                    <div class="form-group">
                                        <label class="col-sm-2 Settinglable  no-margin">注册主体</label>

                                        <div class="col-sm-10">
                                            @Html.TextBoxFor(model => model.PsyRealName, new { @class = "SettingInput col-sm-10 ", @readOnly = "true" })

                                        </div>
                                    </div>
                                </li>


                              




                            </ul>


                        </div>

                    </form>
                </div>
                <!-- /.col -->
            </div>
        </div>
    </div>
    
</div>


@section BottomScripts {
<script type="text/javascript" src="~/plugins/bootbox.min.js"></script>
<script type="text/javascript" src="~/plugins/bootstrap-validator/js/bootstrapValidator.js"></script>


}